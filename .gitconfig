[user]
    name = Allen, Timothy
    email = tallen@wharton.upenn.edu
[color]
    branch = auto
    diff = auto
    status = auto
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red
    new = cyan
[color "status"]
    added = yellow
    changed = green
    untracked = cyan
[merge]
    tool = emerge
[alias]
    co = checkout
    br = branch
    ci = commit
    st = status
    unstage = reset HEAD --
    last = log -1 HEAD
[core]
    autocrlf = true
    editor = 'C:\\Program Files\\Sublime Text 3\\sublime_text.exe' -w
[winUpdater]
    recentlySeenVersion = 2.21.0.windows.1
